{
  "name": "@d2k/react-devto",
  "version": "1.0.0",
  "description": "Dev.to hooks for Github API",
  "main": "lib/react-devto.js",
  "repository": "https://github.com/bdbch/react-devto",
  "author": "@d2k",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dev.to",
    "hooks",
    "github"
  ],
  "scripts": {
    "publish-demo": "rm -rf ./dist && parcel build --public-url /react-devto docs/index.html && npm run gh-pages",
    "publish": "npm run build",
    "build": "rm -rf ./lib && webpack --config ./webpack/config.prod.js",
    "start": "rm -rf ./lib && webpack --config ./webpack/config.dev.js",
    "gh-pages": "gh-pages -d ./dist"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.6",
    "browser-env": "3.2.6",
    "gh-pages": "^2.0.1",
    "parcel": "1.12.2",
    "raf": "3.4.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-test-renderer": "^16.8.4",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
